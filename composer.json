{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
          {
               "type" : "composer",
               "url"  : "https://wpackagist.org"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/gravityforms.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-rocket.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-migrate-db-pro.git"
          }
     ],
     "extra" : {
          "installer-paths" : {
               "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
               "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
          }
     },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
          "landslidecreative/advanced-custom-fields-pro" : "5.12.2",
          "wpackagist-plugin/akismet" : "4.2.2",
          "wpackagist-plugin/classic-editor" : "1.6.2",
          "wpackagist-plugin/featured-images-for-rss-feeds": "1.5.9",
          "landslidecreative/gravityforms" : "2.5.16",
          "wpackagist-plugin/imagify" : "1.10",
          "wpackagist-plugin/intuitive-custom-post-order" : "3.1.3",
          "wpackagist-plugin/jetpack" : "10.8",
          "wpackagist-plugin/safe-redirect-manager" : "1.10.1",
          "wpackagist-plugin/googleanalytics" : "2.5.4",
          "wpackagist-plugin/svg-support" : "2.4.2",
          "wpackagist-plugin/wordfence" : "7.5.9",
          "landslidecreative/wp-migrate-db-pro" : "2.3.2",
          "landslidecreative/wp-rocket" : "3.10.8",
          "wpackagist-plugin/wordpress-seo" : "18.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
